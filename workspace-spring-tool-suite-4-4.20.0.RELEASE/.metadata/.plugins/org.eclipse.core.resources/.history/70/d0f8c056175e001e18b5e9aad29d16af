package com.aziende;

public class AziendaInformatica extends Azienda {

	@Override
	public void produce() {

		System.out.println("produce software/hardware");
	}

	@Override
	public void assumere() {

		System.out.println("assume esperti informatici");
	}

	@Override
	public void fattura() {

		System.out.println("fattura con Iva al 22%");
	}

}
