package com.business;

public class Contatore {

	private static int x;

	public void contatore() {
		x = x + 1;
		System.out.println("valore di x" + x);

	}
	
	/*
	 * un metodo static viene denominato anche
	 * metodo di classe
	 */
	
	public static void contatoreNew() {
		x = x + 1;
		System.out.println("valore di x" + x);
		
		
	}

}
